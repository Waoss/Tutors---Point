<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0" styleClass="anchor-pane"
            stylesheets="@style/MainStyle.css" xmlns="http://javafx.com/javafx/8.0.112"
            fx:controller="com.mnnit.tutorspoint.LoginLayoutController">
    <children>
        <Label layoutX="44.0" layoutY="98.0" text="Username"/>
        <TextField fx:id="usernameTextField" layoutX="142.0" layoutY="94.0"/>
        <Label layoutX="46.0" layoutY="166.0" text="Password"/>
        <PasswordField fx:id="passwordTextField" layoutX="142.0" layoutY="162.0"/>
        <Button fx:id="loginButton" layoutX="46.0" layoutY="210.0" mnemonicParsing="false"
                onAction="#loginButtonOnAction" text="Login"/>
        <Label layoutX="142.0" layoutY="35.0" styleClass="tutors-label" text="Tutors Login"/>
        <Hyperlink fx:id="signUpHyperlink" layoutX="149.0" layoutY="248.0" onAction="#hyperlinkOnAction"
                   text="Already a user! Sign Up!"/>
        <Button fx:id="youtubeButton" layoutX="149.0" layoutY="210.0" mnemonicParsing="false"
                onAction="#youtubeOnAction" text="Watch Youtube Video"/>
    </children>
</AnchorPane>
