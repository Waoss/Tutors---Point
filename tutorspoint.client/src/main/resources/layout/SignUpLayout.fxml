<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" styleClass="anchor-pane"
            stylesheets="@style/MainStyle.css" xmlns="http://javafx.com/javafx/8.0.112"
            fx:controller="com.mnnit.tutorspoint.SignUpLayoutController">
    <children>
        <Label layoutX="44.0" layoutY="98.0" text="Username"/>
        <TextField fx:id="usernameTextField" layoutX="142.0" layoutY="94.0"/>
        <Label layoutX="46.0" layoutY="166.0" text="Password"/>
        <PasswordField fx:id="passwordTextField" layoutX="142.0" layoutY="162.0"/>
        <Button fx:id="signUpButton" layoutX="169.0" layoutY="314.0" mnemonicParsing="false"
                onAction="#signUpButtonOnAction" text="Sign Up"/>
        <Label layoutX="142.0" layoutY="35.0" styleClass="tutors-label" text="Tutors Sign Up"/>
        <RadioButton fx:id="studentRadioButton" layoutX="82.0" layoutY="233.0" mnemonicParsing="false" text="STUDENT"/>
        <RadioButton fx:id="teacherRadioButton" layoutX="257.0" layoutY="233.0" mnemonicParsing="false" text="TEACHER"/>
    </children>

</AnchorPane>
