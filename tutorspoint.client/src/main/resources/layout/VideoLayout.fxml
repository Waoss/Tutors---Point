<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/8.0.112" fx:controller="com.mnnit.tutorspoint.VideoLayoutController">
    <MediaView fx:id="mediaView" fitHeight="400.0" fitWidth="600.0" layoutY="7.0"/>
    <Label fx:id="videoNameLabel" layoutX="14.0" layoutY="399.0" prefHeight="59.0" prefWidth="219.0"
           text="\${videoName}"/>
    <Label fx:id="likesLabel" layoutX="497.0" layoutY="420.0" text="\${likes}"/>
    <Button fx:id="playButton" layoutX="134.0" layoutY="480.0" mnemonicParsing="false" onAction="#playButtonOnAction"
            text="Play"/>
    <Button fx:id="pauseButton" layoutX="194.0" layoutY="480.0" mnemonicParsing="false" onAction="#pauseButtonOnAction"
            text="Pause"/>
    <Button fx:id="stopButton" layoutX="80.0" layoutY="480.0" mnemonicParsing="false" onAction="#stopButtonOnAction"
            text="Stop"/>
    <Label fx:id="timeLabel" layoutX="49.0" layoutY="396.0"/>
    <Slider fx:id="slider" layoutX="8.0" layoutY="382.0" prefHeight="14.0" prefWidth="950.0"/>
    <Button fx:id="toWatch" layoutX="248.0" layoutY="480.0" mnemonicParsing="false" onAction="#toWatchOnAction"
            text="To-Watch"/>
    <Button fx:id="addTag" layoutX="328.0" layoutY="480.0" mnemonicParsing="false" onAction="#addTagOnAction"
            text="Add tag"/>
</AnchorPane>
