<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="700.0"
            xmlns="http://javafx.com/javafx/8.0.112" fx:controller="com.mnnit.tutorspoint.VideoLayoutController">
    <children>
        <MediaView fx:id="mediaView" fitHeight="400.0" fitWidth="600.0" layoutY="7.0"/>
        <Label fx:id="videoNameLabel" layoutX="14.0" layoutY="399.0" prefHeight="59.0" prefWidth="219.0"
               text="\${videoName}"/>
        <Label fx:id="likesLabel" layoutX="497.0" layoutY="420.0" text="\${likes}"/>
        <Button fx:id="playButton" layoutX="274.0" layoutY="408.0" mnemonicParsing="false"
                onAction="#playButtonOnAction"/>
        <Button fx:id="pauseButton" layoutX="326.0" layoutY="408.0" mnemonicParsing="false"
                onAction="#pauseButtonOnAction"/>
        <Button fx:id="stopButton" layoutX="222.0" layoutY="408.0" mnemonicParsing="false"
                onAction="#stopButtonOnAction"/>
        <TextField fx:id="timeLabel" layoutX="49.0" layoutY="396.0"/>
        <Slider fx:id="slider" layoutX="230.0" layoutY="382.0" prefHeight="14.0" prefWidth="425.0"/>
    </children>
</AnchorPane>
